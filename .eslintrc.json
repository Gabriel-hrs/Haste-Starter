{
    "env": {
        "browser": true,
        "jquery": true,
        "es6": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-undef":[
          "warn"
        ],
        "indent": [
            "warn",
            "tab"
        ],
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}
