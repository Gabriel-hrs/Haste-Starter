{
  "name": "haste-starter",
  "version": "1.0.0",
  "description": "[View the documentation](https://bitbucket.org/hastedesign/haste-starter#readme)",
  "main": "webpack.config.js",
  "scripts": {
    "scss": "node-sass --output-style compressed -o dist/css src/scss",
    "autoprefixer": "postcss -u autoprefixer --autoprefixer.browsers '&gt; 5%, ie 9' -r dist/css/*",
    "lint": "eslint src/js",
    "uglify": "mkdir -p dist/js && uglifyjs src/js/*.js -m -c -o dist/js/main.min.js",
    "imagemin": "imagemin src/images dist/images -p",
    "serve": "browser-sync start --server --files 'dist/css/*.css, dist/js/*.js'",
    "build:css": "npm run scss && npm run autoprefixer",
    "build:js": "npm run lint &amp;&amp; npm run uglify",
    "build:images": "npm run imagemin && npm run icons",
    "build:all": "npm run build:css && npm run build:js && npm run build:images",
    "watch:css": "onchange 'src/scss/*.scss' -- npm run build:css",
    "watch:js": "onchange 'src/js/*.js' -- npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://hastedesign@bitbucket.org/hastedesign/haste-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/hastedesign/haste-starter#readme",
  "devDependencies": {
    "bootstrap": "^4.0.0-beta",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "onchange": "^3.2.1",
    "parallelshell": "^3.0.1",
    "popper": "^1.0.0",
    "popper.js": "^1.12.5",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "exports-loader": "^0.6.4"
  }
}
